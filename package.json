{
  "name": "demoTestApp",
  "version": "0.0.1",
  "description": "demoTestApp project with Typescript, Postgres & TypeORM",
  "author": "zeeshan",
  "license": "MIT",
  "scripts": {
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "migration:generate": "ts-node node_modules/typeorm/cli.js migration:generate -f apps/$APP/ormconfig -d apps/$APP/src/migrations -n DbUpdates",
    "migration:run": "ts-node node_modules/typeorm/cli.js migration:run -f apps/$APP/ormconfig",
    "migration:revert": "ts-node node_modules/typeorm/cli.js migration:revert -f apps/$APP/ormconfig",
    "migration:create": "ts-node node_modules/typeorm/cli.js migration:create -f apps/$APP/ormconfig -d apps/$APP/src/migrations -n",
    "schema:drop": "ts-node node_modules/typeorm/cli.js schema:drop -f ormconfig",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint --fix . --ext .ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/demoTestApp/test/jest-e2e.json",
    "compodoc": "npx compodoc -p ./tsconfig.json -s --watch --theme postmark"
  },
  "lint-staged": {
    "*.{ts}": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
